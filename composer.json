{
    "name": "hungthai1401/vulnerable",
    "description": "A Laravel Pulse card to show security vulnerability composer dependencies",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thai Nguyen Hung",
            "email": "hungthai1401.it@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "laravel/pulse": "^1.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "HT\\Pulse\\Vulnerable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HT\\Pulse\\Vulnerable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HT\\Pulse\\Vulnerable\\VulnerableServiceProvider"
            ]
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
